if test "x$with_fast_perl" = "xyes" ; then
  perl -i.bak $ac_aux_dir/conf.change.pl $CONFIG_STATUS || mv $CONFIG_STATUS.bak $CONFIG_STATUS; 
  rm -f $CONFIG_STATUS.bak;
